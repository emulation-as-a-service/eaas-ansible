{
    "Statement": [
        {
            "Principal": { "AWS": ["*"] },
            "Resource": ["arn:aws:s3:::image-archive"],
            "Action": ["s3:GetBucketLocation"],
            "Effect": "Allow"
        },
        {
            "Principal": { "AWS": ["*"] },
            "Resource": ["arn:aws:s3:::image-archive/*"],
            "Action": ["s3:GetObject"],
            "Effect": "Allow"
        }
    ],
    "Version": "2012-10-17"
}